    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^2.0.10",
    "core-js": "^3.8.3",
    "uuid": "^9.0.0",
    "vee-validate": "3",
    "vue": "^2.6.14",
    "vue-jest": "4",
    "vue-loader": "^17.0.1",
    "vuex": "^3.0.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
